{
  "name": "nvse",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-pages": "vite build --base=/nvse/",
    "build-data": "cd lib/nv/ && bash extract.sh && cd ../..",
    "build-icons": "cd lib/nv/ && bash buildcss.sh && cd ../..",
