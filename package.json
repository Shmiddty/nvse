    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vuetify": "^3.5.9"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "vite": "^5.1.5",
    "vite-plugin-vuetify": "^2.0.3"
  }
}
